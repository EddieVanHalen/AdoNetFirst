<UserControl x:Class="AdoTest.View.MyPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AdoTest.View"
             mc:Ignorable="d"
             d:DesignHeight="800" d:DesignWidth="800">
    <Grid>
        <Button Background="White" Command="{Binding BackCommand}" Content="Back" FontSize="15" Width="150" Height="80" Margin="10 10 0 0 " HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="1"></Button>
        <Label VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" FontWeight="1" Content="{Binding Name}"/>
    </Grid>
</UserControl>
